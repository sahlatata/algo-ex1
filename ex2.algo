ALGORITHM Commun 
VAR
    A : ARRAY_OF INTEGER[] := [1,3,4,5]
    B : ARRAY_OF INTEGER[] := [3,7,8,1]
    
    Sum : INTEGER := 0
BEGIN
    
   
    FOR i FROM 1 TO A.length STEP 1  DO
        FOR i FROM 1 TO B.length STEP 1  DO
        IF (A[i]=B[i]) THEN
            sum =: sum + A[i] + B[i]
        END_IF
           
        END_FOR
    END_FOR
 Write(sum) 
END